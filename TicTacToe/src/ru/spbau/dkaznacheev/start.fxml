<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="650.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.spbau.dkaznacheev.StartController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="TicTacToe">
         <font>
            <Font size="50.0" />
         </font></Text>
      <Button mnemonicParsing="false" prefHeight="77.0" prefWidth="228.0" text="Multiplayer" onAction="#handleMultiplayerButtonAction">
         <font>
            <Font size="30.0" />
         </font></Button>
      <Button mnemonicParsing="false" text="Bot Easy" onAction="#handleEasyBotButtonAction">
         <font>
            <Font size="40.0" />
         </font></Button>
      <Button mnemonicParsing="false" text="Bot Hard" onAction="#handleHardBotButtonAction">
         <font>
            <Font size="40.0" />
         </font></Button>
      <Button mnemonicParsing="false" prefHeight="77.0" prefWidth="228.0" text="Log" onAction="#handleLogButtonAction">
         <font>
            <Font size="40.0" />
         </font></Button>

   </children>
</VBox>
